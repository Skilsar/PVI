<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
   <welcome-file-list>
      <welcome-file>pages/index.jsp</welcome-file>
   </welcome-file-list>

   <servlet>
      <servlet-name>Afternoon</servlet-name>
      <servlet-class>Afternoon</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>Afternoon</servlet-name>
      <url-pattern>/Afternoon</url-pattern>
   </servlet-mapping>
   <servlet>
      <servlet-name>Jjj</servlet-name>
      <servlet-class>Jjj</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>Jjj</servlet-name>
      <url-pattern>/Jjj</url-pattern>
   </servlet-mapping>
</web-app>